<?xml version="1.0" encoding="utf-8"?>
<!--This file is added to allow proxy(Charles) support on debug builds-->
<network-security-config>
    <base-config cleartextTrafficPermitted="false" />
    <debug-overrides>
        <trust-anchors>
            <certificates src="user" />
        </trust-anchors>
    </debug-overrides>
</network-security-config>